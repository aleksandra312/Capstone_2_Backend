{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.comment.com/commentNew.schema.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "createDate": {
      "type": "string",
      "format": "date"
    },
    "usState": {
      "type": "string",
      "minLength": 5,
      "maxLength": 100
    },
    "comment": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "isRelocate": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": ["username", "createDate", "comment", "isRelocate"]
}
